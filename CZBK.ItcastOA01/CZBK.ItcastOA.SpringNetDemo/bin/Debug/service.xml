<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">
  <description>An  example that demonstrates simple IoC features.</description>
  <object name="UserInfoService" type="CZBK.ItcastOA.SpringNetDemo.UserInfoService, CZBK.ItcastOA.SpringNetDemo">
    <property name="UserName" value="zhangsan"/>
    <property name="Person" ref="MyPerson"/>
  </object>
  <object name="MyPerson"  type="CZBK.ItcastOA.SpringNetDemo.Person,CZBK.ItcastOA.SpringNetDemo">
    <property name="Age" value="18"/>
  </object>
</objects>